                    <div pref-code="hideGoogleAds" access-value="true">
                        <div parent-id="Content/US/EN/lotsearchresult.html" data-cms-content-fragment="" fragment-id="Google-Ads1"><!----><div ng-bind-html="fragmentedCms"><script>
        var userstat = 'logged-out';
        var lang = 'eng';
        var breakpoint = 'desktop';
        var test = 'Searchresult';
        var REFRESH_KEY = 'refresh';
        var REFRESH_VALUE = 'true';
        var refresh_rate = 60;

        googletag.cmd.push(function() {
            var leftMapping = googletag.sizeMapping().
                              addSize([768, 0], [160, 600]).
                              addSize([0, 0], []).
                              build();

            googletag.defineSlot('/35265761/SearchResults_160x600', [160, 600], 'div-gpt-ad-1450241013744-0')
                .defineSizeMapping(leftMapping)
                .addService(googletag.pubads())
                .setTargeting("pos", "bottom")
                .setTargeting(REFRESH_KEY, REFRESH_VALUE);          

            googletag.pubads().addEventListener('impressionViewable', function(event) {
              var slot = event.slot;
              if (slot.getTargeting(REFRESH_KEY).indexOf(REFRESH_VALUE) > -1) {
                setTimeout(function() {
                  googletag.pubads().refresh([slot]);
                }, refresh_rate * 1000);
              }
            });
            
            googletag.pubads().setTargeting("userstat",userstat);
            googletag.pubads().setTargeting("lang",lang);
            googletag.pubads().setTargeting("breakpoint",breakpoint);
            googletag.pubads().setTargeting("test",test);

            googletag.pubads().collapseEmptyDivs();
            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
        });
    </script>
